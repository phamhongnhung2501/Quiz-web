{
  "name": "appstack-react",
  "version": "1.2.2",
  "description": "AppStack - ReactJS Admin & Dashboard Template",
  "author": "Paul Laros <support@bootlab.io>",
  "private": true,
  "scripts": {
    "sass:compile": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "react-scripts build",
    "watch:css": "chokidar \"src/assets/scss/**/*\" -c \"npm run build:css\"",
    "watch:js": "react-scripts start",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.9.3",
    "apexcharts": "3.7.1",
    "availity-reactstrap-validation": "2.5.3",
    "axios": "^0.19.2",
    "bootstrap": "^4.3.1",
    "chart.js": "2.9.4",
    "chokidar-cli": "^2.1.0",
    "classnames": "2.2.6",
    "fullcalendar": "3.10.0",
    "google-map-react": "1.1.4",
    "i18next": "^17.0.4",
    "i18next-browser-languagedetector": "^3.0.1",
    "jquery": "3.4.1",
    "jvectormap-next": "3.0.0",
    "moment": "2.24.0",
    "node-sass": "^4.13.1",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "4.1.5",
    "postcss-cli": "6.1.2",
    "react": "^16.8.6",
    "react-apexcharts": "1.3.3",
    "react-bootstrap-table-next": "3.1.3",
    "react-bootstrap-table2-filter": "^1.3.0",
    "react-bootstrap-table2-paginator": "2.0.6",
    "react-bootstrap-table2-toolkit": "2.0.0",
    "react-chartjs-2": "2.7.6",
    "react-cookie": "^4.0.0",
    "react-datetime": "2.16.3",
    "react-day-picker": "^7.4.0",
    "react-dom": "16.8.6",
    "react-dragula": "1.1.17",
    "react-feather": "1.1.6",
    "react-i18next": "^10.11.2",
    "react-icons": "^3.9.0",
    "react-input-mask": "2.0.4",
    "react-perfect-scrollbar": "1.5.3",
    "react-popper": "1.3.3",
    "react-quill": "1.3.3",
    "react-redux": "7.0.3",
    "react-redux-toastr": "7.5.1",
    "react-resize-image": "^1.0.4",
    "react-router-auth": "^1.0.1",
    "react-router-dom": "5.0.0",
    "react-scripts": "^3.3.1",
    "react-select": "3.0.4",
    "react-transition-group": "4.1.0",
